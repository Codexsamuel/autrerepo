  "scripts": {
    "build": "next build",
    "build:frontend": "NEXT_PUBLIC_API_URL=https://votre-backend-api.vercel.app next build",
    "build:backend": "next build",
    "build:static": "NEXT_CONFIG_FILE=next.config.static.js next build",
    "clean": "rm -rf .next node_modules && pnpm install",
    "db:migrate": "node scripts/database-migration.js",
    "db:seed": "node scripts/database-seed.js",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:vercel": "vercel --prod",
    "deploy:frontend": "pnpm run build:frontend && netlify deploy --prod --dir=.next",
    "deploy:backend": "pnpm run build:backend && vercel --prod",
    "deploy:static": "pnpm run build:static && netlify deploy --prod --dir=out",
    "dev": "next dev",
    "lint": "next lint",
    "reset": "rm -rf .next node_modules package-lock.json pnpm-lock.yaml && pnpm install",
    "scrape:aliexpress": "node scripts/aliexpress-scraper.js",
    "start": "next start",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "type-check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]

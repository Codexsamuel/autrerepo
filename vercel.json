{
  "version": 2,
  "buildCommand": "echo 'Backend build completed'",
  "installCommand": "npm ci",
  "framework": null,
  "ignoreCommand": "echo 'Skipping build'",
  "builds": [
    {
      "src": "api-backup/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api-backup/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api-backup/index.ts"
    }
  ],
  "functions": {
    "api-backup/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}

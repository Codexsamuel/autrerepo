{
  "static": {
    "maxAge": 31536000,
    "immutable": true,
    "headers": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "api": {
    "maxAge": 300,
    "staleWhileRevalidate": 60,
    "headers": {
      "Cache-Control": "public, max-age=300, stale-while-revalidate=60"
    }
  },
  "images": {
    "maxAge": 86400,
    "headers": {
      "Cache-Control": "public, max-age=86400"
    }
  },
  "fonts": {
    "maxAge": 31536000,
    "headers": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  }
}
# Configuration Supabase (Backend - Vercel)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Configuration Clerk (Authentification)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# Configuration Stripe (Paiements)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Configuration IA
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Configuration MetaAPI (Trading)
METAAPI_TOKEN=your_metaapi_token_here
METAAPI_ACCOUNT_ID=your_metaapi_account_id_here

# Configuration Google Cloud (Traduction)
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id_here
GOOGLE_CLOUD_PRIVATE_KEY=your_google_cloud_private_key_here

# URLs de l'application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=https://your-backend-api.vercel.app
NEXT_PUBLIC_NETLIFY_DEPLOY_URL=your_netlify_deploy_url_here
NEXT_PUBLIC_VERCEL_URL=your_vercel_url_here

# Configuration environnement
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# Base de données (optionnel)
DATABASE_URL=postgresql://username:password@host:port/database

# Redis (optionnel)
REDIS_URL=redis://localhost:6379 

# Configuration de déploiement
NETLIFY_SITE_ID=your_netlify_site_id_here
VERCEL_PROJECT_ID=your_vercel_project_id_here 
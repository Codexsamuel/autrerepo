{
  "name": "davy-trading-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:setup": "bash scripts/setup-database.sh --auto",
    "db:create": "bash scripts/setup-database.sh --db",
    "db:migrate": "bash scripts/setup-database.sh --migrate",
    "db:seed": "bash scripts/setup-database.sh --seed",
    "db:backup": "bash scripts/setup-database.sh --backup",
    "db:verify": "bash scripts/setup-database.sh --verify",
    "db:reset": "npm run db:backup && bash scripts/setup-database.sh --auto",
    "docker:build": "docker build -t davy-trading-platform .",
    "docker:run": "docker run -p 3000:3000 davy-trading-platform",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.7",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/supabase-js": "^2.39.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "framer-motion": "^11.0.3",
    "lucide-react": "^0.323.0",
    "next": "14.2.25",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "recharts": "^2.10.4",
    "stripe": "^14.17.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.41.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.25",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/davy-trading-platform.git"
  },
  "keywords": [
    "trading",
    "ai",
    "nextjs",
    "typescript",
    "supabase",
    "clerk",
    "stripe"
  ],
  "author": "DAVY Trading Team",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
